      <TextField fx:id="txtName" editable="false" layoutX="564.0" layoutY="207.0" prefHeight="30.0" prefWidth="190.0" />

      <Label fx:id="lblPhoneNumber" layoutX="367.0" layoutY="256.0" text="Phone Number :">
