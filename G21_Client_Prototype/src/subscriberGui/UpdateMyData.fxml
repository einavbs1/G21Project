      <TextField fx:id="txtEmail" editable="true" layoutX="562.0" layoutY="306.0" prefHeight="30.0" prefWidth="190.0" />

      <Label fx:id="lblPassword" layoutX="368.0" layoutY="355.0" text="Password :">
