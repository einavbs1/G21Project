            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="txtPhoneNumber" editable="true" layoutX="563.0" layoutY="255.0" prefHeight="30.0" prefWidth="190.0" />

      <Label fx:id="lblEmail" layoutX="366.0" layoutY="306.0" text="Email Address :">
