            <Font size="18.0" />
         </font></Label>
      <TextField fx:id="txtPassword" editable="true" layoutX="562.0" layoutY="353.0" prefHeight="30.0" prefWidth="190.0" />

      <Label fx:id="lblStatus" layoutX="368.0" layoutY="403.0" text="Status :">
