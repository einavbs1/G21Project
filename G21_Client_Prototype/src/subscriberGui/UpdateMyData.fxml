            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button fx:id="btnExit" layoutX="1005.0" layoutY="656.0" mnemonicParsing="false" onAction="#getExitBtn" prefHeight="30.0" prefWidth="50.0" text="Exit" textFill="#b81212">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>

      <Label id="lblMessage" fx:id="lblMessage" alignment="CENTER" layoutX="392.0" layoutY="558.0" prefHeight="59.0" prefWidth="320.0" textAlignment="CENTER" textFill="#478150" wrapText="true">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Label id="lblTitle" fx:id="lblTitle" alignment="CENTER" layoutX="18.0" layoutY="15.0" prefHeight="32.0" prefWidth="1066.0" text="Update My Data">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <Label id="lblErrMessage" fx:id="lblErrMessage" alignment="CENTER" layoutX="395.0" layoutY="558.0" prefHeight="59.0" prefWidth="320.0" textFill="#b81212">
