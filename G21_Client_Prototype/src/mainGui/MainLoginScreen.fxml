<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="234.0" prefWidth="292.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainControllers.MainLoginScreenController">
   <children>
      <Button id="btnExit" fx:id="btnExit" layoutX="121.0" layoutY="177.0" mnemonicParsing="false" onAction="#getExitBtn" text="Exit" />
