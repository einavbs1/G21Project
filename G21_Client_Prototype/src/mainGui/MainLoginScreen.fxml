      <Button id="btnSubscriberLogin" fx:id="btnSubscriberLogin" layoutX="160.0" layoutY="72.0" mnemonicParsing="false" onAction="#SubscriberButton" text="Login as Subscriber" />
      <Button id="btnLibrarianLogin" fx:id="btnLibrarianLogin" layoutX="14.0" layoutY="72.0" mnemonicParsing="false" onAction="#LibrarianButton" text="Login as Librarian" />
      <Button id="btnGuestLogin" fx:id="btnGuestLogin" layoutX="86.0" layoutY="118.0" mnemonicParsing="false" onAction="#GuestButton" text="Continue as a guest" />
   </children>
</AnchorPane>
