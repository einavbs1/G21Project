<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="626.0" prefWidth="1114.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SearchBookResultsController">
   <children>
      <Button id="btnExit" fx:id="btnExit" layoutX="1045.0" layoutY="582.0" mnemonicParsing="false" onAction="#getExitBtn" text="Exit" />
      <Button id="btnBack" fx:id="btnBack" layoutX="43.0" layoutY="582.0" mnemonicParsing="false" onAction="#Back" text="Back" />
      <TableView id="tableViewAvailableBooks" fx:id="tableViewAvailableBooks" layoutX="23.0" layoutY="43.0" prefHeight="211.0" prefWidth="1078.0">
        <columns>
          <TableColumn fx:id="columnBookBarcode1" prefWidth="88.0" text="book_barcode" />
          <TableColumn fx:id="columnBookTitle1" prefWidth="122.0" text="book_title" />
            <TableColumn fx:id="columnBookSubject1" prefWidth="94.0" text="book_subject" />
            <TableColumn fx:id="columnBookDescription1" minWidth="0.0" prefWidth="558.0" text="book_description" />
            <TableColumn fx:id="columnBookAvilableCopies1" minWidth="0.0" prefWidth="130.0" text="book_availableCopies" />
            <TableColumn fx:id="columnBookLocation1" prefWidth="85.0" text="book_location" />
        </columns>
      </TableView>
      <TableView id="tableViewUnavailableBooks" fx:id="tableViewUnavailableBooks" layoutX="25.0" layoutY="321.0" prefHeight="200.0" prefWidth="1078.0">
        <columns>
            <TableColumn fx:id="columnBookBarcode2" prefWidth="86.0" text="book_barcode" />
          <TableColumn fx:id="columnBookTitle2" prefWidth="118.0" text="book_title" />
          <TableColumn fx:id="columnBookSubject2" prefWidth="92.0" text="book_subject" />
            <TableColumn fx:id="columnBookDescription2" prefWidth="384.0" text="book_description" />
            <TableColumn fx:id="columnBookCanOrder2" prefWidth="250.0" text="book_canOrder" />
            <TableColumn fx:id="columnBookClosestReturnDate2" prefWidth="147.0" text="book_closestReturnDate" />
        </columns>
      </TableView>
      <Label alignment="CENTER" layoutX="242.0" layoutY="14.0" prefHeight="25.0" prefWidth="715.0" text="Available matches books">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="284.0" layoutY="288.0" prefHeight="17.0" prefWidth="557.0" text="Unavailable matches books">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="285.0" layoutY="254.0" prefHeight="17.0" prefWidth="545.0" text="You can find your book at the library with the location and borrow it with the librarian." />
      <Label layoutX="181.0" layoutY="527.0" text="Books that matched your search description but not available to borrow. You can order the books if your book's column contains can order" />
      <Label layoutX="368.0" layoutY="544.0" text="Don't forget to save the book's barcode to order the book you want." wrapText="true" />
      <Button id="btnOrderBook" fx:id="btnOrderBook" layoutX="526.0" layoutY="582.0" mnemonicParsing="false" onAction="#OrderBook" text="Order Book" />
   </children>
</AnchorPane>
